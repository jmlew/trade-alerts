{
  "name": "trade-alerts",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "----- SERVE --------------------": "",
    "start": "npm run serve:api & npm run serve:web",
    "serve:web": "kill:web & nx run web-app:serve",
    "serve:api": "kill:api & nx run mock-api:serve",
    "serve:dist": "live-server dist/apps/web-app --no-browser --port=6600",
    "----- BUILD --------------------": "",
    "build": "nx build",
    "build:prod": "nx build --prod",
    "----- TEST --------------------": "",
    "test": "nx test",
    "e2e": "nx e2e web-app-e2e",
    "e2e:watch": "nx e2e web-app-e2e --watch",
    "e2e:prod": "nx e2e web-app-e2e --configuration=production",
    "----- CODE --------------------": "",
    "format": "prettier --ignore-path .gitignore --write .",
    "lint": "nx affected --target=lint",
    "----- MISC --------------------": "",
    "storybook": "nx storybook web-app",
    "storybook:build": "nx build-storybook web-app",
    "graph": "nx graph",
    "prepare": "husky install",
    "npmremove": "rm -rf node_modules",
    "clean:install": "npm run npmremove && npm cache verify && npm install",
    "kill:web": "kill -9 $(lsof -t -i tcp:4200)",
    "kill:api": "kill -9 $(lsof -t -i tcp:3333)",
    "kill:storybook": "kill -9 $(lsof -t -i tcp:4400)"
  },
