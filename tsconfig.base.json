      ],
      "@trade-alerts/alert-manager/shell": ["libs/alert-manager/shell/src/index.ts"],
      "@trade-alerts/dashboard/api": ["libs/dashboard/api/src/index.ts"],
      "@trade-alerts/dashboard/domain": ["libs/dashboard/domain/src/index.ts"],
      "@trade-alerts/dashboard/feature/alert-manager-drawer": [
        "libs/dashboard/feature/alert-manager-drawer/src/index.ts"
      ],
