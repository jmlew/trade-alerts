      "@trade-alerts/shared/util-filters": ["libs/shared/util-filters/src/index.ts"],
      "@trade-alerts/shared/util-form": ["libs/shared/util-form/src/index.ts"],
      "@trade-alerts/users/domain": ["libs/users/domain/src/index.ts"],
      "@trade-alerts/users/feature/manage-users": [
        "libs/users/feature/manage-users/src/index.ts"
      ],
      "@trade-alerts/users/shell": ["libs/users/shell/src/index.ts"],
